{
    "name": "feelin",
    "version": "0.0.1",
    "description": "",
    "author": "Hyeonseok Jung",
    "private": true,
    "license": "UNLICENSED",
    "type": "module",
    "scripts": {
        "prebuild": "rimraf dist",
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "nest start -e \"node --loader esm-module-alias/loader\"",
        "start:dev": "nest start --watch -e \"node --loader esm-module-alias/loader\"",
        "start:debug": "nest start --debug --watch -e \"node --loader esm-module-alias/loader\"",
        "start:prod": "node --loader esm-module-alias/loader dist/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "jest",
        "test:exit": "jest --detectOpenHandles --forceExit",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "clean": "rm -rf dist",
        "clean:module": "rm -rf node_modules",
        "clean:all": "rm -rf dist node_modules",
        "typeorm": "node --loader esm-module-alias/loader -r ts-node/register ./node_modules/typeorm/cli.js --config dist/ormConfig.js",
        "migration:create": "yarn typeorm migration:create",
        "migration:run": "yarn typeorm migration:run",
        "migration:revert": "yarn typeorm migration:revert"
    },
    "dependencies": {
        "@grpc/grpc-js": "^1.6.12",
        "@grpc/proto-loader": "^0.7.2",
        "@nestjs/common": "^9.0.11",
        "@nestjs/config": "^2.2.0",
        "@nestjs/core": "^9.0.11",
        "@nestjs/jwt": "^9.0.0",
        "@nestjs/passport": "^9.0.0",
        "@nestjs/platform-express": "^9.0.11",
        "@nestjs/swagger": "^6.1.1",
        "@nestjs/typeorm": "^9.0.1",
        "@types/cheerio": "^0.22.30",
        "@types/node-rsa": "^1.1.1",
        "@types/puppeteer": "^5.4.5",
        "@types/random-useragent": "^0.3.1",
        "axios": "^0.27.2",
        "cheerio": "^1.0.0-rc.10",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.13.2",
        "esm-module-alias": "^2.0.1",
        "lodash-es": "^4.17.21",
        "mysql2": "^2.3.3",
        "n-gram": "^2.0.1",
        "node-fetch": "^3.2.6",
        "node-rsa": "^1.1.1",
        "passport": "^0.6.0",
        "passport-jwt": "^4.0.0",
        "pg-mem": "^2.6.4",
        "puppeteer": "^17.1.0",
        "random-useragent": "^0.5.0",
        "rimraf": "^3.0.2",
        "rxjs": "^7.2.0",
        "swagger-ui-express": "^4.3.0",
        "typeorm": "^0.3.9",
        "watchman": "^1.0.0"
    },
    "aliases": {
        "@": "dist",
        "@feelin-types": "dist/types",
        "@utils": "dist/utils"
    },
    "devDependencies": {
        "@babel/core": "^7.18.10",
        "@babel/plugin-transform-modules-commonjs": "^7.18.6",
        "@babel/preset-env": "^7.18.10",
        "@nestjs/cli": "^9.1.1",
        "@nestjs/schematics": "^9.0.2",
        "@nestjs/testing": "^9.0.11",
        "@types/express": "^4.17.13",
        "@types/jest": "29.0.2",
        "@types/lodash-es": "^4.17.6",
        "@types/node": "^18.7.14",
        "@types/passport-jwt": "^3.0.7",
        "@types/supertest": "^2.0.11",
        "@typescript-eslint/eslint-plugin": "^5.36.1",
        "@typescript-eslint/parser": "^5.36.1",
        "babel-jest": "^29.0.2",
        "eslint": "^8.0.1",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "jest": "^29.0.2",
        "prettier": "^2.3.2",
        "source-map-support": "^0.5.20",
        "sqlite3": "^5.0.11",
        "supertest": "^6.1.3",
        "ts-jest": "^29.0.2",
        "ts-loader": "^9.2.3",
        "ts-node": "^10.0.0",
        "typescript": "^4.3.5",
        "typescript-transform-paths": "^3.3.1"
    }
}
